#!/bin/sh

GRAPH=$1

echo "##########"
echo "########## CLASSIFYING $GRAPH WITH WEKA"
echo "##########"

./run-graphutils-arff $1 $1.test.paths.txt
mv $GRAPH.arff $GRAPH.test.arff

./run-graphutils-arff $1 $1.train.paths.txt
mv $GRAPH.arff $GRAPH.train.arff

./run-wekads $GRAPH

./run-wekarf $GRAPH 100 0
